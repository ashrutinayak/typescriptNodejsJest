    "prebuild": "rimraf dist/*",
    "start": "node dist/index.js",
    "start:dev": "nodemon dist/index.js",
    "dev": "nodemon ./src/index.ts",
    "test": "jest --testMatch='**/test/**/*.[jt]s?(x)'"
